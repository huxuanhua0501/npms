@charset "UTF-8";
.fs-nav{
    width: 100%;
    height: 30px;
    line-height: 30px;
    background-color: #EBF4FD;
    border: #EEEFF0 solid 1px;
}
.fs-path{
    padding: 0px 3px 0px 0px;
    cursor: pointer;
}
.fs-path::before{
    content: "/";
    padding-right: 5px;
    text-underline: none;
}
.fs-title{
    height: 25px;
    line-height: 25px;
    position: relative; z-index: 1;}
.fs-title-text{
    position: absolute; z-index: 3; background-color: white; padding-right: 5px;}
.fs-title-line{
    height: 13px;
    border-bottom: #E5ECF0 solid 1px;
    width: 100%;
    position: absolute; z-index: 2;}
.folder-ul{
    list-style: none;
    margin: 0px;
    padding: 8px 0px;
}
.folder-li{
    width: 275px;
    height: 70px;
    list-style: none;
    padding: 0px;
    margin: 0px 10px 7px 0px;
    float: left;
    border: white solid 1px;
}
.folder-li:hover{
    border: #E5ECF0 solid 1px;
    background-color: #FCFCFC;
    cursor: pointer;
}
.folder-li-pic{
    float: left;
    height: 50px;
    width: 50px;
    margin-top: 12px;
    margin-left: 12px;
}
.folder-li-pic-bg1{
    background: url("../../images/c01.png");
}
.folder-li-pic-bg2{
    background: url("../../images/c02.png");
}
.folder-li-pic-bg3{
    background: url("../../images/c03.png");
}
.folder-li-info{
    width: 202px;
    height: 62px;
    float: right;
    padding: 5px 5px 0px 0px;
}
.folder-progress{
    width: 190px;
    height: 15px;
    border: #DAE3E8 solid 1px;
    margin: 5px 0px;
}
.folder-progress-scale{
    /*width: 50%;*/
    height: 100%;
    background-color: #7DC84A;
}
.fs-icon{
    width: 16px;
    height: 16px;
    display: inline-block;
    vertical-align: text-bottom;
}
.fs-icon-folder{
    background: url("../../images/f01.png");
}
.fs-icon-zip{
    background: url("../../images/f02.png");
}
.fs-icon-txt{
    background: url("../../images/f03.png");
}
.fs-icon-ai{
    background: url("../../images/f04.png");
}
.fs-icon-excel{
    background: url("../../images/f05.png");
}
.fs-icon-other{
    background: url("../../images/f06.png");
}